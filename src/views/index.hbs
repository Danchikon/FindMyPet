    <section class="list-items">
        <div class="container-full">
            <div class="container">
                <h3>All pets</h3>
            </div>
            <div class="container">
                <ul class="element-list four-per-row">
                    <li class="grid-el column">
                        {{#if products.length}}
                            {{#each products}}
                            {{{ post }}}
                            {{/each}}
                        {{/if}}
                    </li>
                </ul>
            </div>
            <div class="container">
                <ul class="pagination row vertical-center">
                    <li class="active"><a class="center-all" href="#">1</a></li>
                    <li class=""><a class="center-all" href="#">2</a></li>
                    <li class=""><a class="center-all" href="#">3</a></li>
                    <li class=""><a class="center-all" href="#">4</a></li>
                    <li class=""><a class="center-all" href="#">5</a></li>
                </ul>
            </div>
        </div>
    </section>
    <footer class="footer">
