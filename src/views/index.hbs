    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/slick-carousel/1.8.1/slick.min.js"></script>
    <script>
        $('.footer-slider').slick({
            infinite: true,
            autoplay: true,
            autoplaySpeed: 0,
            cssEase: 'linear',
            speed: 4000,
            slidesToShow: 6,
            slidesToScroll: 1,
            arrows: false,
            dragble: false,
            responsive: [
                {
                    breakpoint: 600,
                    settings: {
                    slidesToShow: 3,
                    slidesToScroll: 1
                    }
                },
                {
                    breakpoint: 768,
                    settings: {
                    slidesToShow: 5,
                    slidesToScroll: 1
                    }
                }

                ]
        }); 
    </script>
